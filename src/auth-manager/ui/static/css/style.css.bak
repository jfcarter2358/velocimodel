/* shadows */
.moc-shadow {box-shadow:0 2px 5px 0 rgba(0,0,0,0.16),0 2px 10px 0 rgba(0,0,0,0.12);}
.moc-vertical-shadow {box-shadow: 0px 2px #444444;}
.moc-horizontal-shadow {box-shadow: 2px 0px #444444;}
.moc-diagonal-shadow {box-shadow: 2px 2px #444444;}
.moc-vertical-shadow-grey {box-shadow: 0px 2px #aaaaaa;}
.moc-horizontal-shadow-grey {box-shadow: 2px 0px #aaaaaa;}
.moc-diagonal-shadow-grey {box-shadow: 2px 2px #aaaaaa;}

/* primary brand colors */
.moc-green,.moc-hover-green:hover{color:#fff!important;background-color:#10B26C!important}
.moc-pastel-green,.moc-hover-pastel-green:hover{color:#000!important;background-color:#c1ead6!important}
.moc-light-green,.moc-hover-light-green:hover{color:#000!important;background-color:#6ecc6e!important}
.moc-dark-green,.moc-hover-dark-green:hover{color:#fff!important;background-color:#03331e!important}

/* secondary brand colors */
.moc-blue-green,.moc-hover-blue-green:hover{color:#fff!important;background-color:#075f68!important}
.moc-orange,.moc-hover-orange:hover{color:#000!important;background-color:#f9b341!important}
.moc-light-blue,.moc-hover-light-blue:hover{color:#000!important;background-color:#68c3d8!important}
.moc-blue,.moc-hover-blue:hover{color:#fff!important;background-color:#2d6cd3!important}
.moc-yellow,.moc-hover-yellow:hover{color:#000!important;background-color:#e5c645!important}
.moc-forest-green,.moc-hover-forest-green:hover{color:#fff!important;background-color:#478400!important}
.moc-red,.moc-hover-red:hover{color:#000!important;background-color:#ea5c5c!important}
.moc-olive-green,.moc-hover-olive-green:hover{color:#000!important;background-color:#a2b290!important}

.moc-grey,.moc-hover-grey:hover{color:#000!important;background-color:#aaaaaa!important}

